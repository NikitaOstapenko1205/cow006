{
  "manifest_version": 3,
  "name": "BGA game helper",
  "description": "Extension for calculate cards in '6 nimmt', show game info in all games",
  "version": "0.0.4",
  "author": "Mykyta Ostapenko",
  "icons": {},
  "content_scripts": [
    {
      "matches": ["https://boardgamearena.com/*"],
      "js": ["inject.js"],
      "css": ["style.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["content.js"],
      "matches": ["https://boardgamearena.com/*"]
    }
  ]
}
